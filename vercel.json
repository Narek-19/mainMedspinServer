{
  "version": 2,
  "builds": [
    {
      "src": "./src/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "./public/image/(.*)",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
  },
    {
      "src": "/(.*)",
      "dest": "src/",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    }
  ],
  "outputDirectory": "staticfiles_build"
}
