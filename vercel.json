{
  "version": 2,
  "builds": [
    {
      "src": "./src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/public/image/(.*)",
      "dest": "/public/image/$1",
      "headers": { "Cache-Control": "public, max-age=86400" },
      "methods": ["GET"]
    },
    {
      "src": "public/topCourses.json",
      "dest": "public/topCourses.json",
      "headers": { "Cache-Control": "public, max-age=86400" },
      "methods": ["GET"]
    },
    {
      "src": "/(.*)",
      "dest": "src/",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    }
  ]
}
